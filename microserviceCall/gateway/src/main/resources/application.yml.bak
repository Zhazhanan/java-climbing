server:
  port: 10014

logging:
  level:
    org.springframework.cloud.gateway: DEBUG

spring:
  application:
    name: gateway
  cloud:
    nacos:
      discovery:
        server-addr: 127.0.0.1:8848
        group: DEFAULT_GROUP
        username: nacos
        password: nacos
    gateway:
      routes:
        - id: customerID
          uri: lb://nacos-customer
          predicates:
            - Path=/api/**
          filters:
            - StripPrefix=1
        - id: producerID
          uri: lb://nacos-producer
          predicates:
            - Path=/doc/**
          filters:
            - StripPrefix=1
            - name: IpFilter
              args:
                whiteList: 127.0.0.1,127.0.1.1,127.0.1.2,127.0.1.3